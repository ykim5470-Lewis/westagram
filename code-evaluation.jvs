function divideArrayInHalf(array) {
  let result = [];
  if (array[4]<=10) {
    result.unshift(array[4]);
    } else if (array[4]>10) {
      result.push(array[4]);
    }
  if (array[3]<=10) {
    result.unshift(array[3]);
    } else if (array[3]>10) {
      result.push(array[3]);
    }
  if  
    (array[2]<=10) {
    result.unshift(array[2]);
    } else if (array[2]>10) {
      result.push(array[2]);
    }
  if
    (array[1]<=10) {
    result.unshift(array[1]);
    } else if (array[1]>10) {
      result.push(array[1]);
    }
  if
    (array[0]<=10) {
    result.unshift(array[0]);
    } else if (array[0]>10) {
      result.push(array[0]);
    }
    return result;
}

console.log(divideArrayInHalf([20,10,1,3,4]));
